{
    "name": "exenjer/laravel-filters",
    "description": "Easy and convenient filters for your Laravel Eloquent application",
    "keywords": [
        "laravel",
        "eloquent",
        "filter"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "ExenJer",
            "email": "exenjers@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ExenJer\\LaravelFilters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExenJer\\LaravelFilters\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "illuminate/pagination": "5.8.*",
        "illuminate/database": "5.8.*",
        "illuminate/support": "6.10.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7"
    }
}
